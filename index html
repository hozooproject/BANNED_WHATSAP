  <script>  
    // Demo interaksi tombol BLOCK  
    document.getElementById('blockBtn').addEventListener('click', () => {  
      const phoneField = document.getElementById('phone');  
      const full = `+62 08${phoneField.value.replace(/\D/g,'')}`;  
      alert(`(Demo) Nomor akan diblokir: ${full}`);  
    });  

    // Refresh button
    document.getElementById('refreshBtn').addEventListener('click', () => {
      location.reload();
    });

    // Connect button functionality
    document.getElementById('connectBtn').addEventListener('click', () => {
      const phoneField = document.getElementById('phone');
      const phoneNumber = phoneField.value.replace(/\D/g,'');
      
      if (phoneNumber.length === 10) {
        const fullNumber = `+62 08${phoneNumber}`;
        document.getElementById('connectedNumber').textContent = fullNumber;
        document.getElementById('overlay').classList.add('show');
        document.getElementById('fakeChat').classList.add('show');
        
        // WhatsApp connection functionality
        window.open(`https://wa.me/${fullNumber.replace(/\D/g,'')}`, '_blank');
      } else {
        alert('Masukkan nomor yang valid (10 digit setelah +62 08)');
      }
    });

    // Close fake chat
    document.getElementById('closeChat').addEventListener('click', () => {
      document.getElementById('overlay').classList.remove('show');
      document.getElementById('fakeChat').classList.remove('show');
    });

    // JARVIS sound effect
    document.getElementById('jarvisImage').addEventListener('click', () => {
      const jarvisSound = document.getElementById('jarvisSound');
      jarvisSound.currentTime = 0;
      jarvisSound.play();
      
      // Using SpeechSynthesis API for voice
      if ('speechSynthesis' in window) {
        const utterance = new SpeechSynthesisUtterance();
        utterance.text = "Hello Lordhozoo, what can I help you with? I want to attack people. Gas kan!";
        utterance.rate = 0.9;
        utterance.pitch = 0.8;
        speechSynthesis.speak(utterance);
      } else {
        alert("JARVIS: Hello Lordhozoo, what can I help you with? I want to attack people. Gas kan!");
      }
    });
  </script>  
</body>  
</html>
